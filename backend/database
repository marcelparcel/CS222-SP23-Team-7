CREATE DATABASE majors ;
CREATE TABLE IF NOT EXISTS mytable (
    id INT(10) NOT NULL PRIMARY KEY AUTO_INCREMENT,
    major VARCHAR(200) NOT NULL,
    credits VARCHAR(200),
    college VARCHAR(200),
    courses VARCHAR(200) NOT NULL
);
